<div <div class="container-full">
<%= render 'layouts/header' %>
      <div class="row">
       
        <div class="col-lg-12 text-center v-center">
          
          <h1>Generic Template Login</h1>
          <p class="lead">Landing page for generic template</p>
          
          <br />
          
          <form class="col-lg-12">
            <%= form_for(:session, url: login_path) do |f| %>
             
                <%= f.label :email %><br />
                <%= f.email_field :email, class: "input-group input-group-lg col-sm-offset-4 col-sm-4" %><br />
              
              
                <%= f.label :password %>
                <%= link_to "(forgot password)", new_password_reset_path %><br />
                <%= f.password_field :password, class: "input-group input-group-lg col-sm-offset-4 col-sm-4" %><br />
            
            	  <%= f.label :remember_me do %>
                <%= f.check_box :remember_me %>
                <span>Remember me on this computer</span><br /><br />
                <% end %>
                <div class="center">  
                <%= f.submit "Login", class: "btn btn-lg btn-primary" %><br />
                <% end %>
                </div><br />
                  <p>New user? <%= link_to "Sign up now!", signup_path %></p>
          </form>
        </div>
        
